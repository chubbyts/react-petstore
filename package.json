    "@tanstack/react-query": "^5.90.12",
    "cross-fetch": "^4.1.0",
    "date-fns": "^4.1.0",
    "qs": "^6.14.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.10.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@chubbyts/chubbyts-function-mock": "^2.0.2",
    "@eslint/js": "^9.39.2",
    "@prettier/sync": "^0.6.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.2",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.2.7",
